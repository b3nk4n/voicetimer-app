      <ListenFor>Beende [Timer]</ListenFor>
      <ListenFor>Stopp [Timer]</ListenFor>
      <ListenFor>[Timer] Abbrechen</ListenFor>
      <ListenFor>Breche [Timer] ab</ListenFor>
      <Feedback>Ich schalte den Timer aus</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

    <Command Name="checkAlarmTime">
      <Example>Auf wann ist der Timer gestellt?</Example>
      <ListenFor>[Auf] wann ist der Timer [gestellt] [gesetzt]?</ListenFor>
      <Feedback>Ich schaue nach</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

    <Command Name="checkRemainingTime">
      <Example>Wie viel Zeit habe ich noch?</Example>
      <ListenFor>Wie viel Zeit habe ich noch?</ListenFor>
      <ListenFor>Wie viel Zeit ist noch übrig?</ListenFor>
      <ListenFor>Restzeit?</ListenFor>
      <ListenFor>Verbleibende Zeit?</ListenFor>
      <ListenFor>Wie lange noch?</ListenFor>
      <Feedback>Ich schaue nach</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

    <Command Name="extendAlarmTime1">
      <Example>plus 5 Minuten</Example>
      <ListenFor>plus {minute} [Minuten]</ListenFor>
      <ListenFor>[um] {minute} [Minuten] verlängern</ListenFor>
      <ListenFor>{minute} [Minuten] mehr</ListenFor>
      <ListenFor>Verlängern um {minute} [Minuten]</ListenFor>
      <Feedback>Verlängere Timer um {minute} Minuten</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

    <Command Name="extendAlarmTime2">
      <Example>plus 1 Stunde und 5 Minuten</Example>
      <ListenFor>plus {hour} Stunde [und] {minute} [Minuten]</ListenFor>
      <ListenFor>[um] {hour} Stunde [und] {minute} [Minuten] verlängern</ListenFor>
      <ListenFor>{hour} Stunde [und] {minute} [Minuten] mehr</ListenFor>
      <ListenFor>Verlängern um {hour} Stunde [und] {minute} [Minuten]</ListenFor>
      <Feedback>Verlängere Timer um {hour} Stunde und {minute} Minuten</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

