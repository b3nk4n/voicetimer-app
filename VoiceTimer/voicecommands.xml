  <CommandSet xml:lang="en-GB" Name="commandsENGB">

    <CommandPrefix>Timer</CommandPrefix>
    <Example>for 1 hour and 20 minutes</Example>

    <Command Name="startTimer1">
      <Example>for 20 minutes</Example>
      <ListenFor>[for] {minute} minutes</ListenFor>
      <Feedback>Turning alarm on</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

    <Command Name="startTimer1b">
      <Example>for 1 hour</Example>
      <ListenFor>[for] {hour} hour</ListenFor>
      <Feedback>Turning alarm on</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>

    <Command Name="startTimer1c">
      <Example>for 30 seconds</Example>
      <ListenFor>[for] {second} seconds</ListenFor>
